<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <WorkGiverDef>
        <defName>OperateHeldHuman</defName>
        <label>operate on held humans</label>
        <giverClass>HeldHuman.Def.WorkGiver_.WorkGiver_OperateHeldHuman</giverClass>
        <workType>Doctor</workType>
        <priorityInType>70</priorityInType>
        <verb>operate on</verb>
        <gerund>operating on</gerund>
        <requiredCapacities>
            <li>Manipulation</li>
        </requiredCapacities>
        <prioritizeSustains>true</prioritizeSustains>
        <billGiversAllHumanlikes>true</billGiversAllHumanlikes>
        <billGiversAllHumanlikesCorpses>true</billGiversAllHumanlikesCorpses>
    </WorkGiverDef>

    <WorkGiverDef>
        <defName>FeedHeldHuman</defName>
        <label>feed held humans</label>
        <giverClass>HeldHuman.Def.WorkGiver_.WorkGiver_FeedHeldHuman</giverClass>
        <workType>Warden</workType>
        <verb>feed</verb>
        <gerund>feeding</gerund>
        <priorityInType>80</priorityInType>
        <requiredCapacities>
            <li>Manipulation</li>
        </requiredCapacities>
    </WorkGiverDef>
